<p><div class="page-header"><h2>Aircraft models</h2><div>
<div class="row">

  </div></p>

{{# each products}}
<div class="col-sm-5 col-md-2">
<div class="thumbnail" >
     <img src="{{ imagePath }}"  class="img-responsive" >
      <div class="caption">
        <h3> {{ title }}</h3>
        <div class="clearfix">
        <div id='{{_id}}'class="price">&#8377; {{ price }} 
<script> var price={{price}}+'';
         if(price.length>3){
         var lastThree = price.substring(price.length-3);
         var otherNumbers = price.substring(0,price.length-3);
         if(otherNumbers != '')
                lastThree= ',' +lastThree;
         var res = otherNumbers.replace(/\B(?=(\d{2})+(?!\d))/g,",") + lastThree;
         document.getElementById('{{ _id }}').innerHTML='&#8377;'+res;}</script>

</div>
        <a href="/addtoaccart/{{ this._id}}" class="btn btn-primary pull-right" role="button"> Add to cart</a>
</div>
</div>
</div>
    </div>
{{/each}}
</div>
<div class="btn-group">
<a href="/acparts/{{ this._id}}" class="btn btn-primary pull-right" role="button"> Buy MIG 21 parts</a>
<a href="/acp2/{{ this._id}}" class="btn btn-primary pull-right" role="button"> Buy Kiran parts</a>
<a href="/acp3/{{ this._id}}" class="btn btn-primary pull-right" role="button"> Buy Canberra parts</a>
<a href="/acp4/{{ this._id}}" class="btn btn-primary pull-right" role="button"> Buy Dhruv parts</a>
<a href="/acp5/{{ this._id}}" class="btn btn-primary pull-right" role="button"> Buy Pushpak parts</a>
</div>



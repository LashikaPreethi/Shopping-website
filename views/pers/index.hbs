<div class="page-header">Personal care<div>
<div class="row">

  </div>
{{# each products}}
<div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="{{ imagePath }}" class="img-responsive">
      <div class="caption">
        <h3> {{ title }}</h3>
        <div class="clearfix">
        <div class="price"> {{ price }} </div>
        <a href="/addtopcart/{{this._id}}" class="btn btn-primary pull-right   " role="button">Add to cart</a>
</div>
</div>
</div>
    </div>

{{/each}}
 </div>

